{
    "name": "coffreo/php-translation-js-extractor-bundle",
    "description": "Add javascript translation extractor for php-translation/symfony-bundle package",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Cyril MERY",
            "email": "cmery@coffreo.com"
        }
    ],
    "keywords": [
        "js",
        "javascript",
        "translation",
        "translate",
        "i18n",
        "php-translation",
        "js-translation",
        "bazinga"
    ],
    "suggest": {
        "willdurand/js-translation-bundle": "Allow usage of translated strings in Javascript"
    },
    "scripts": {
        "test": "vendor/bin/simple-phpunit --coverage-text --coverage-html=build/coverage",
        "cs": "vendor/bin/php-cs-fixer fix --diff --verbose"
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "php-translation/extractor": "^2.0.2",
        "symfony/framework-bundle": "^3.0 || ^4.3 || ^5.0",
        "php-translation/symfony-bundle": ">=0.9.0 <1.0.0",
        "coffreo/js-translation-extractor": "^0.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4 || ^4.0",
        "coffreo/php-cs-fixer-config": "^2.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Coffreo\\PHPTranslationJsExtractorBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Coffreo\\PHPTranslationJsExtractorBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
